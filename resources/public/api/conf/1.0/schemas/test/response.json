{
  "title": "A JSON Schema for TTP API.",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "requestId": {
      "type": "string",
      "description": "request id"
    },
    "content": {
      "type": "string",
      "description": "content"
    },
    "uri": {
      "type": "string",
      "description": "uri"
    },
    "isResponse": {
      "type": "boolean",
      "description": "isResponse"
    },
    "status": {
      "type" : "number",
      "description": "statusCode"
    }
  }
}