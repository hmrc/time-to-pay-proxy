{
  "title": "A JSON Schema for TTP API.",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "customerReference": {
      "type": "string",
      "minLength": 1,
      "maxLength": 15,
      "description": "The Unique Customer reference"
    },
    "planId": {
      "type": "string",
      "minLength": 1,
      "maxLength": 15,
      "description": "Unique planId"
    }
  },
  "required": [
    "customerReference",
    "planId"
  ]
}